{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "signalr@2.4.1",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "jquery.signalR.min.js",
        "jquery.signalR.js"
      ]
    },
    {
      "library": "@microsoft/signalr@3.1.3",
      "destination": "wwwroot/js/signalr/",
